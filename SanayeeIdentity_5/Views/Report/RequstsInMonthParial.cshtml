@model List<SanayeeIdentity_5.Data.RequstsInMonthOfYear_Result>
<div class="row panel-body">
    
 

    @for (int i = 0; i < Model.Count; i++)
    {
        if (i == 0)
        {
            <h4 style="color:red">  @Model[i].year </h4>  
        }

        <div class="col-lg-3 panel " style="border-bottom :2px dashed #4CAF50  ">
            <progress value="@Model[i].numofrequsts" max="@ViewBag.MaxReq"></progress>
            <br />
            <h6 style="background-color:yellow" class="img-thumbnail"> <b> @Model[i].month</b></h6>


            <h6 class="img-thumbnail"><b> @Model[i].numofrequsts </b></h6>

        </div>
    }
   

</div>
